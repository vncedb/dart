{
  "expo": {
    "name": "DART",
    "slug": "dart-app",
    "owner": "project-vdb",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "dartapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.projectvdb.dart",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "notification": {
      "icon": "./assets/images/icon/notification-icon.png",
      "color": "#4F46E5"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#F1F5F9"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.projectvdb.dart"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/dart-logo-transparent-dark.png",
          "resizeMode": "contain",
          "backgroundColor": "#f9f9f9",
          "imageWidth": 200,
          "dark": {
            "image": "./assets/images/dart-logo-transparent-light.png",
            "backgroundColor": "#0f172a"
          }
        }
      ],
      "expo-web-browser",
      [
        "expo-dev-client",
        {
          "launchMode": "most-recent"
        }
      ],
      "expo-audio",
      "expo-sqlite",
      "./plugins/withNotificationIcons"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "b87b2b51-77da-47f5-930f-5539a1304dc0"
      }
    }
  }
}
